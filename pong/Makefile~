CC	=	gcc

RM	=	rm -f

CFLAGS	+=	
CFLAGS	+=	-I./include

LDFLAGS	=

NAME	=	pong

SRCS	=	main.c

OBJS	=	$(SRCS:.c=.o)

all	:	$(NAME)

$(NAME)	:	$(OBJS)
		$(CC) -o $(NAME) $(OBJS) $(CFLAGS) $(LDFLAGS)

clean	:	
		$(RM) $(OBJS)

fclean	:	clean
		$(RM) $(NAME)

re	:	fclean all

.PHONY	:	fclean all re clean
